import java.util.ArrayList;

public class Histogram {
   public static ArrayList<Integer> DiceRoll() {
      ArrayList<Integer> frequencies;
      frequencies = new ArrayList<Integer>();
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      frequencies.add(0);
      

      
      for ( int i = 0; i < 1000; i++)
      {
         Dice dice = new Dice();
         dice.roll(); 

         frequencies.set( dice.getDiceTotal() - 2, );

      }
 }
}